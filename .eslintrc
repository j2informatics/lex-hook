{
    "root": true,
        "parser": "@typescript-eslint/parser",
        "plugins": [
          "@typescript-eslint"
        ],
        "extends": [
          "eslint:recommended",
          "plugin:@typescript-eslint/eslint-recommended",
          "plugin:@typescript-eslint/recommended"
        ],
    "rules": {
        "max-len": [ "error", { "code": 120 }],
        "@typescript-eslint/no-namespace": 0,
        "@typescript-eslint/no-unused-vars": 1
    }
}